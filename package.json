    "imagemin-svgo": "^7.0.0",
    "next": "^8.0.3",
    "next-compose-plugins": "^2.1.1",
    "next-i18next": "^0.36.5",
    "next-optimized-images": "^2.4.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.5",
